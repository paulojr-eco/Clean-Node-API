{
  "name": "clean-node-api",
  "version": "2.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist tsconfig-build.tsbuildinfo && tsc -p tsconfig-build.json",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "test": "vitest --passWithNoTests --silent --no-threads",
    "test:unit": "npm test -- --config vitest-unit.config.ts",
    "test:integration": "npm test -- --config vitest-integration.config.ts",
    "test:ci": "npm test -- run --coverage --no-threads",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:staged": "vitest run --passWithNoTests --no-threads",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^20.5.1",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitest/coverage-istanbul": "^0.33.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "lint-staged": "^13.2.3",
    "mockdate": "^3.0.5",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0",
    "vitest-mongodb": "^0.0.5"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "husky": "^8.0.3",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "mongodb": "^5.7.0",
    "nodemon": "^3.0.1",
    "validator": "^13.9.0"
  },
  "engines": {
    "node": "20.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
